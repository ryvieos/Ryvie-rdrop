{
  "expo": {
    "name": "rDrop",
    "slug": "rDrop",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/apple-touch-icon.png",
    "scheme": "rdrop",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.julesmaisonnave.rDrop",
      "buildNumber": "2",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Cette application nécessite l'accès à votre photothèque pour y sauvegarder les images téléchargées.",
        "NSPhotoLibraryAddUsageDescription": "Cette application nécessite l'accès à votre photothèque pour y sauvegarder les images téléchargées.",
        "UIBackgroundModes": ["fetch"],
        "UIViewControllerBasedStatusBarAppearance": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ],
      "package": "com.julesmaisonnave.rDrop"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-media-library",
        {
          "photosPermission": "Autorisez l'accès à vos photos pour sauvegarder les images téléchargées.",
          "savePhotosPermission": "Autorisez l'application à sauvegarder les photos dans votre galerie.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5ac32088-286a-46b7-ae6b-e2073effc997"
      }
    }
  }
}
